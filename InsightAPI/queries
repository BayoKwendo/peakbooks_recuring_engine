
      CREATE TABLE Insight.estimate_items (
           id int(11) NOT NULL AUTO_INCREMENT,
           estimate_no varchar(100)  NULL,
           item_id varchar(100)  NULL,
           name varchar(100)  NULL,
           price varchar(100)  NULL,
           description varchar(1000) NULL,
           quantity varchar(1000) NOT NULL,
       PRIMARY KEY (id)
       )
       
    
          CREATE TABLE Insight.estimate_items (
           id int(11) NOT NULL AUTO_INCREMENT,
           estimate_no varchar(100)  NULL,
           customer_id varchar(100)  NULL,
           expirary_date varchar(100)  NULL,
           estimate_date varchar(100)  NULL,
           estimate_message varchar(1000) NULL,
           statement_message varchar(1000) NOT NULL,
       PRIMARY KEY (id)
       )